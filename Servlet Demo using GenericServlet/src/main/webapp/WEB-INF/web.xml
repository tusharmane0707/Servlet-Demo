<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  <servlet-name>SignUp</servlet-name>
  <servlet-class>com.jsp.demo.SignUp</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>SignUp</servlet-name>
  <url-pattern>/signup</url-pattern>
  </servlet-mapping>


  <servlet>
  <servlet-name>Login</servlet-name>
  <servlet-class>com.jsp.demo.Login</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>Login</servlet-name>
  <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>Button</servlet-name>
  <servlet-class>com.jsp.demo.Button</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>Button</servlet-name>
  <url-pattern>/button</url-pattern>
  </servlet-mapping>
</web-app>